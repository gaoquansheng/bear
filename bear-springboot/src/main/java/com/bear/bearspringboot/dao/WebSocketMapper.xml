<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bear.bearspringboot.dao.WebSocketMapper">

    <insert id="saveMessage" parameterType="Message">
        insert into message (sendUserTel,message,receivedUserTel,sendTime) values
         (#{sendUserTel},#{message},#{receivedUserTel},#{sendTime})
    </insert>
</mapper>